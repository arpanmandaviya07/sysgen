{
  "name": "arpanmandaviya/laravel-system-builder",
  "description": "Generate Laravel migrations, models, controllers, and views from a minimal JSON schema (Laravel 12+)",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Arpanmandaviya\\SystemBuilder\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arpanmandaviya\\SystemBuilder\\SystemBuilderServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "symfony/finder": "^7.0"
  },
  "scripts": {
    "sys": "php artisan system:build --help"
  }
}